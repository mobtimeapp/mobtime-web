{
  "name": "@mobtimeapp/web",
  "version": "0.0.0-beta.0",
  "description": "MobTime web front-end built with HyperApp and Vite.",
  "author": "Alex Barry",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "tailwind": "tailwindcss build -i ./src/styles/index.css -o ./src/styles/styles.css -c ./tailwind.config.js",
    "test": "ava",
    "dev": "vite",
    "build": "vite build --emptyOutDir"
  },
  "keywords": [
    "mobtimeapp",
    "mobtime",
    "web",
    "hyperapp"
  ],
  "dependencies": {
    "ava": "^4.3.3",
    "hyperapp": "^2.0.22",
    "sinon": "^14.0.1",
    "tailwindcss": "^3.1.8",
    "vite": "^3.1.4"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "concurrency": 8,
    "failFast": true,
    "failWithoutAssertions": true
  }
}
